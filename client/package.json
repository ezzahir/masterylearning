    "d3": "^3.5.17",
    "elearning-content-fmi": "git+ssh://git@gitlab.com/lanoxx/elearning-content-fmi.git#master",
    "jquery": "^2.2.4",
    "js-utils": "git+ssh://git@gitlab.com/lanoxx/js-utils.git#master",
    "lodash": "^4.17.4",
    "text-clipper": "^1.2.1",
    "vis": "^4.16.1"
  }
